# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type User {
  id: ID!
  name: String!
}

type Product {
  id: ID!
  user_id: ID!
  name: String!
  price: Int!
}

type Query {
  getUser(id: Int!): User!
  getUsers(id: Int, name: String): [User!]!
  getProduct(id: Int!): Product!
  getProducts(id: Int, user_id: Int, name: String, price: Int): [Product!]!
}
